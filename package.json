{
  "name": "farce",
  "version": "0.2.8",
  "description": "History repeats itself",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib --delete-dir-on-start",
    "lint": "eslint examples/*/src src test *.js",
    "prepublish": "npm run build",
    "tdd": "cross-env NODE_ENV=test karma start",
    "test": "npm run lint && npm run testonly",
    "testonly": "npm run tdd -- --single-run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Catalyzer/farce.git"
  },
  "keywords": [
    "history"
  ],
  "author": "4Catalyzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4Catalyzer/farce/issues"
  },
  "homepage": "https://github.com/4Catalyzer/farce#readme",
  "dependencies": {
    "dom-helpers": "^5.1.2",
    "invariant": "^2.2.4",
    "is-promise": "^2.1.0",
    "query-string": "^5.1.1",
    "redux": "^4.0.4",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@4c/babel-preset": "^7.0.0",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "cross-env": "^6.0.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.1.0",
    "eslint-config-4catalyzer": "^1.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.3",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^9.2.1",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.39.2"
  }
}
